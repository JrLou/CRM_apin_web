{
  "entry": "src/index.js",
  "outputPath": "./server/dist",
  "extraBabelPlugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "externals": {
    "g2": "G2",
    "g-cloud": "Cloud",
    "g2-plugin-slider": "G2.Plugin.slider"
  },
  "proxy": {
     "/java": {
    "target": "http://appadmin.pre.io.apin.com/",
    "changeOrigin": true,
    "pathRewrite": { "^/java" : "" }
    },
     "/": {
    "target": "http://appadmin.test.io.apin.com/",
    "changeOrigin": true
    }

  },
  "ignoreMomentLocale": true,
  "theme": "./src/theme.js",
  "hash": true
}
