{
  "entry": "src/index.js",
  "outputPath": "./server/dist",
  "extraBabelPlugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "externals": {
    "g2": "G2",
    "g-cloud": "Cloud",
    "g2-plugin-slider": "G2.Plugin.slider"
  },
  "proxy": {
    "/api": {
    "target": "http://10.5.101.12:6040/",
    "changeOrigin": true,
    "pathRewrite": { "^/api" : "/api" }
    }
  },
  "ignoreMomentLocale": true,
  "theme": "./src/theme.js",
  "hash": true
}
